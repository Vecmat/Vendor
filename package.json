{
    "name": "@vecmat/vendor",
    "version": "0.0.3",
    "description": "",
    "author": "Hanrea",
    "license": "ISC",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "require": "./dist/index.js",
        "import": "./dist/index.mjs"
    },
    "scripts": {
        "clean": "rm -rf dist",
        "test": "yarn build:test && npx mocha test",
        "build": "yarn clean && yarn compile && npx gulp copy",
        "compile": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
        "deploy": "yarn build && npm publish ./dist --registry https://npm.vecmat.cc",
        "watch": "concurrently \"tsc -w --project tsconfig.json\" \"tsc-alias -w --project tsconfig.json\""
    },
    "dependencies": {
        "lodash": "~4.17.21",
        "mathjs": "^11.5.0",
        "moment": "^2.29.4",
        "murmurhash": "^2.0.1",
        "uuid": "~9.0.0",
        "@types/lodash": "~4.14.195"
    },
    "devDependencies": {
        "@types/node": "~18.14.6",
        "concurrently": "~7.4.0",
        "gulp": "^4.0.2",
        "gulp-add-src": "^1.0.0",
        "gulp-cli": "^2.3.0",
        "tslib": "~2.4.0",
        "typescript": "~4.9.5",
        "tsc-alias": "~1.8.2"
    },
    "nodemonConfig": {
        "ignore": [
            "test/*",
            "docs/*",
            "wait/*"
        ],
        "watch": [
            "./"
        ],
        "execMap": {
            "ts": "ts-node"
        },
        "legacyWatch": true,
        "delay": "1000"
    }
}
